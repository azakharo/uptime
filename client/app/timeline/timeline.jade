uib-progress
  uib-bar(ng-repeat='intervl in timeIntervals track by $index', value='intervl.percent', type='{{intervl.color}}',
  uib-tooltip-html="getTooltipHtml(intervl)", tooltip-append-to-body="true", tooltip-placement="bottom")
    span(ng-hide='intervl.percent < 5') {{intervl.percent | number:1}}%
